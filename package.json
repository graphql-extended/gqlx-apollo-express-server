{
  "name": "gqlx-apollo-express-server",
  "version": "0.6.0",
  "description": "A Node.js Express middleware for integrating an Apollo server supporting gqlx.",
  "main": "dist",
  "types": "dist",
  "scripts": {
    "start": "ts-node src/example.ts",
    "test:unit": "jest src",
    "test": "npm run lint && npm run test:unit",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --config prettier.config.js --write 'src/**/*.ts'",
    "build": "tsc --project tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphql-extended/gqlx-apollo-express-server.git"
  },
  "keywords": [
    "graphql",
    "express",
    "middleware",
    "schema"
  ],
  "author": "graphql-extended <gqlx@anglevisions.com>",
  "engines": {
    "node": ">=12.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphql-extended/gqlx-apollo-express-server/issues"
  },
  "homepage": "https://graphql-extended.github.io",
  "dependencies": {
    "apollo-server-express": "^2.25.3",
    "gqlx-js": "^0.3.0",
    "graphql": "^16.0.0",
    "graphql-subscriptions": "^1.2.1",
    "graphql-type-json": "^0.2.4",
    "subscriptions-transport-ws": "^0.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/graphql-type-json": "^0.1.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^9.6.61",
    "@types/ws": "^6.0.4",
    "express": "^4.17.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "prettier": "^1.19.1",
    "rimraf": "^2.7.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^8.8.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.2.0",
    "typescript": "^4.6.2"
  }
}
